<body>
  <div id="FID" style="width: 500px; height: 500px;"></div>

  <div id="FCP" style="width: 500px; height: 500px;"></div>

  <div id="TTFB" style="width: 500px; height: 500px;"></div>

  <div id="CLS" style="width: 500px; height: 500px;"></div>

  <div id="LCP" style="width: 500px; height: 500px;"></div>
</body>
<script>
  const FID = [], FCP = [], TTFB = [], CLS = [], LCP = [];
  for (let index = 0; index < dataSource.length; index++) {
    const data = dataSource[index];
    const [performanceType, hour, location] = data.dimensionValues;
    const [value, count] = data.metricValues;
    const average = value.value / count.value;

    switch (performanceType.value) {
      case 'FID':
        FID.push({ hour, location, average });
        break;
      case 'FCP':
        FCP.push({ hour, location, average });
        break;
      case 'TTFB':
        TTFB.push({ hour, location, average });
        break;
      case 'CLS':
        CLS.push({ hour, location, average });
        break;
      case 'LCP':
        LCP.push({ hour, location, average });
        break;
      default:
        break;
    }


  };

  createchart('FID', FID);
  createchart('FCP', FCP);
  createchart('TTFB', TTFB);
  createchart('CLS', CLS);
  createchart('LCP', LCP);
  function createchart(id, dimention) {
    var chartDom = document.getElementById(id);
    var myChart = echarts.init(chartDom);
    let legends = dimention.map(item => item.location.value);
    //  remove duplicate
    legends = [...new Set(legends)];
    let series = [];
    // group by location
    legends.forEach(legend => {
      const data = dimention.filter(item => item.location.value === legend);
      series.push({
        name: legend,
        type: 'line',
        data: data.map(item => item.average)
      })
    });


    var option;
    console.log(legends, series);
    option = {
      title: {
        text: id
      },
      tooltip: {},
      legend: {
        data: legends
      },
      xAxis: {
        data: FID.map(item => item.hour.value)
      },
      yAxis: {},
      series
    };
    option && myChart.setOption(option);
  }
</script>
